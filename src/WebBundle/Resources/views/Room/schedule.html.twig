{% extends 'WebBundle:layouts:main.html.twig' %}

{% block content %}
    <!--  CONTENT -->
    <div class="content container-fluid">

        <div class="row room genesis">
            <img src="{{ asset('img/genesis-img.jpg') }}" alt="{{ room.title }}" class="room-images">

            <div class="room-left-panel">
                <img src="{{ asset('img/genesis-logo.png') }}" alt="{{ room.title }}" class="room-logo">
                <h3 class="room-title">{{ room.title }}</h3>
                <p class="room-complexity">{{ 'Complexity'|trans }} 3</p>
                <div class="room-description">{{ room.description|raw }}</div>
                <p class="team">{{ 'Players'|trans }} {{ room.playersMin }}-{{ room.playersMax }}</p>
                <p class="time-to-play">{{ room.timeMax }} {{ 'minutes' }}</p>
            </div>

            <a href="#" class="team-results">{{ 'Team results'|trans }}</a>

            {#<p class="average-time">{{ 'Average time' }} <span></span> {{ 'minutes'|trans }}</p>#}

            <div class="room-right-panel">
                <p><span class="glyphicon glyphicon-phone" aria-hidden="true"></span> &nbsp;{{ room.phone }}</p>
                <p><span class="glyphicon glyphicon-envelope" aria-hidden="true"></span> &nbsp;{{ room.email }}</p>
                <p><i class="fa fa-map-marker" aria-hidden="true"></i> {{ room.address }}</p>
            </div>
        </div>

        <section class="schedule">
            <h2 class="text-center">{{ 'Quest schedule'|trans }}</h2>
            <div class="shedule-slider">
                <ul class="slides">
                    {% for sevenDays in schedule %}
                        <li>
                            <div class="table-responsive">
                                <table class="table">
                                    <thead>
                                    <tr>
                                        <th></th>
                                        {% for game in sevenDays[0]['dates'] %}
                                            <th class="{% if 'now'|date('d.m.Y') == game.date|date('d.m.Y') %} th-today cell-today{% endif %}{% if game.date|date('l') in ['Saturday', 'Sunday'] %} th-weekend{% endif %}">
                                                <span class="font-s">{% if 'now'|date('d.m.Y') == game.date|date('d.m.Y') %}{{ 'Today'|trans }}{% endif %}</span>
                                                <br>
                                                {{ game.date|date('d') }} {{ game.date|date('F')|trans }}
                                                <br>
                                                <span class="font-s">{{ game.date|date('l')|trans }}</span>
                                            </th>
                                        {% endfor %}
                                    </tr>
                                    </thead>
                                    <tbody>
                                    {% for row in sevenDays %}
                                        <tr>
                                            <td class="cell-time">{{ row.time }}</td>
                                            {% for game in row.dates %}
                                                <td class="{% if 'now'|date('d.m.Y') == game.date|date('d.m.Y') %} cell-today{% endif %}"
                                                    data-date-time="{{ game.date }} {{ row.time }}"
                                                    data-prices="{{ game.prices|json_encode }}">
                                                    {% if game.busy %}
                                                        <div class="cell cell-expired">
                                                            <span class="game-time">{{ row.time }}</span>
                                                            - {{ 'from'|trans }}
                                                            <br>
                                                            <span class="game-price">{{ game.prices|first.price }}</span> {{ room.currency }}
                                                        </div>
                                                    {% else %}
                                                        {% if game.prices|first.price < 600 %}
                                                            {% set priceClass = 'price-xs' %}
                                                        {% elseif game.prices|first.price >= 600 and game.prices|first.price < 700 %}
                                                            {% set priceClass = 'price-s' %}
                                                        {% elseif game.prices|first.price >= 700 and game.prices|first.price < 800 %}
                                                            {% set priceClass = 'price-m' %}
                                                        {% elseif game.prices|first.price >= 800 %}
                                                            {% set priceClass = 'price-l' %}
                                                        {% endif %}
                                                        <div class="cell {{ priceClass }}">
                                                            <span class="game-time">{{ row.time }}</span>
                                                            - {{ 'from'|trans }}
                                                            <br>
                                                            <span class="game-price">{{ game.prices|first.price }}</span> {{ room.currency }}
                                                        </div>
                                                    {% endif %}
                                                </td>
                                            {% endfor %}
                                        </tr>
                                    {% endfor %}
                                    </tbody>
                                </table>
                            </div>
                        </li>
                    {% endfor %}
                </ul>
            </div>
        </section>

        <section class="form-reservation">
            <h2 class="text-center">{{ 'Book game'|trans }}</h2>
            <div class="row greeting-form">
                <div class="col-sm-12 text-center">
                    <h3>{{ room.title }}</h3>
                    <img src="{{ asset('img/genesis-logo.png') }}" alt="{{ room.title }}" class="room-logo">
                    <p>
                        <i class="li-ico fa fa-exclamation"></i>
                        <span>{{ 'Please, select a game'|trans }}.</span>
                    </p>
                </div>
            </div>
            <div class="booking-form">
                <div class="row">
                    <div class="col-sm-4 text-center">
                        <h3>{{ room.title }}</h3>
                        <img src="{{ asset('img/genesis-logo.png') }}" alt="{{ room.title }}" class="room-logo">
                    </div>
                    <div class="col-sm-4">
                        <ul>
                            <li>
                                <i class="li-ico fa fa-map-marker"></i>
                                <span>{{ room.address }}</span>
                            </li>
                            <li>
                                <i class="li-ico fa fa-calendar"></i>
                                <span id="date">--.--.----</span>
                            </li>
                            <li>
                                <i class="li-ico fa fa-clock-o"></i>
                                <span id="time">--:--</span>
                            </li>
                            <li>
                                <i class="li-ico fa fa-money"></i>
                                <span id="price">0</span> {{ room.currency }}
                            </li>
                            <li>
                                <i class="li-ico fa fa-exclamation"></i>
                                <span class="font-s">{{ 'Pre-booking is required'|trans }}.</span>
                            </li>
                        </ul>
                    </div>
                    <div class="col-sm-4 text-center">
                        <div>
                            <input type="text" name="name" id="name" required placeholder="{{ 'Name'|trans }}*">
                        </div>
                        <div>
                            <input type="text" name="lastName" id="lastName" required
                                   placeholder="{{ 'Last Name'|trans }}*">
                        </div>
                        <div>
                            <input type="email" name="email" id="email" required placeholder="Email*">
                        </div>
                        <div>
                            <input type="tel" name="phone" id="phone" required
                                   placeholder="{{ 'Phone'|trans }}*">
                        </div>
                        <div>
                            <select required id="players">
                                <option disabled selected>{{ 'Players'|trans }}*</option>
                                <option value="5">5</option>
                            </select>
                        </div>
                        <div class="input-group">
                            <input type="text" name="discount" id="discount"
                                   placeholder="{{ 'Discount'|trans }}">
                            <span class="input-group-btn">
                                        <button class="btn btn-warning" type="button">{{ 'Use discount' }}</button>
                                    </span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="text-center btn-wrap">
                        <button type="submit" class="btn btn-warning">{{ 'Book game'|trans }}</button>
                    </div>
                </div>
            </div>
            <div class="row result-form">
                <div class="col-sm-4">
                    <h3 class="text-center">{{ 'Congratulations'|trans }}!</h3>
                    <p class="color-orange text-center">{{ 'You have successfuly booked the game'|trans }}</p>
                    <p>Мы свяжемся с Вами ближайшее время, по указанному Вами номеру телефона</p>:
                    <p>Все детали Вашего заказа мы отправили на</p>:
                </div>
                <div class="col-sm-4 form-middle">
                    <h3 class="text-center">{{ 'Your order'|trans }}:</h3>
                    <p class="color-orange text-center">{{ 'Quest'|trans }} - {{ room.title }}</p>
                    <ul>
                        <li>
                            <i class="fa fa-map-marker"></i>
                            <span>{{ room.address }}</span>
                        </li>
                        <li>
                            <i class="fa fa-user"></i>
                            <span>players</span>
                        </li>
                    </ul>
                </div>
                <div class="col-sm-4">
                    <h3 class="text-center">{{ 'Price'|trans }}</h3>
                    <p class="text-center"><span>800</span> {{ room.currency }}</p>
                    <p>{{ 'Our contacts'|trans }}:</p>
                    <ul>
                        <li>
                            <i class="fa fa-phone"></i>
                            <span>{{ room.phone }}</span>
                        </li>
                        <li>
                            <i class="fa fa-envelope"></i>
                            <span>{{ room.email }}</span>
                        </li>
                    </ul>
                </div>
            </div>
        </section>

